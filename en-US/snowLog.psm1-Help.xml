			<command:example>
				<maml:title>EXAMPLE 2: USE OF GLOBAL VARIABLES</maml:title>
				<maml:introduction>
					<maml:para />
				</maml:introduction>
				<dev:code># Set Variables
  $importantFile = 'C:\Users\&lt;user&gt;\Desktop\important.zip'
  $destinationFile = '\\SERVER\Share\Important.zip'
  $global:objectName = $importantFile

##### Start Copy Process ##### 
  write-snowLog -logName 'File Copy Process' -action 'Start Process' -status 'Start'
  $sourceHash = Get-Hash $importantFile 
  write-snowLog -logName 'File Copy Process' -action 'Get Source Hash' -status 'Complete' -hash $sourceHash.hashString -object $sourceHash.Algorithm

  write-snowLog -logName 'File Copy Process' -action 'Copy Files' -status 'Start'
  copy-item -path $importantFile -Destination $destinationFile
  write-snowLog -logName 'File Copy Process' -action 'Copy Files' -status 'End'

  $destinationHash = Get-Hash $destinationFile 
  write-snowLog -logName 'File Copy Process' -action 'Get Destination Hash' -status 'Completed' -hash $destinationHash.hashString -object $destinationHash.Algorithm -objectName $destinationFile
##### End Copy Process ##### 

